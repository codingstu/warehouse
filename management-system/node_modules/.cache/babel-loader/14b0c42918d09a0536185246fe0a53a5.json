,EAAA,IAAA,CAAA,SAAA,CAAA,MAAA,CAAA,GAAA;AAAA,iBAAA,CAFA;;AAAA;AAAA;AAEA,gBAAA,IAFA,mBAEA,IAFA;;AAGA,oBAAA,IAAA,KAAA,CAAA,EAAA;AACA,kBAAA,MAAA,CAAA,eAAA,GADA,CACA;;AACA;;AALA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,OALA,IAYA,KAZA,CAYA,YAAA;AACA,QAAA,MAAA,CAAA,QAAA,CAAA;AACA,UAAA,IAAA,EAAA,MADA;AAEA,UAAA,OAAA,EAAA;AAFA,SAAA;AAIA,OAjBA;AAkBA,KA3FA;AA4FA;AACA,IAAA,UA7FA,sBA6FA,GA7FA,EA6FA;AACA;AACA,WAAA,QAAA,qBAAA,GAAA,EAFA,CAEA;AACA;;AACA,WAAA,iBAAA,GAAA,IAAA;AACA,KAlGA;AAmGA;AACA,IAAA,QApGA,sBAoGA;AAAA;;AAAA;AAAA;;AAAA;AAAA;AAAA;AAAA;AACA,gBAAA,MAAA,CAAA,iBAAA,GAAA,KAAA,CADA,CACA;AACA;;AAFA,kCAGA,MAAA,CAAA,QAHA,EAGA,EAHA,mBAGA,EAHA,EAGA,YAHA,mBAGA,YAHA,EAGA,aAHA,mBAGA,aAHA;AAAA;AAAA,uBAIA,YAAA,CAAA;AAAA,kBAAA,EAAA,EAAA,EAAA;AAAA,kBAAA,YAAA,EAAA,YAAA;AAAA,kBAAA,aAAA,EAAA;AAAA,iBAAA,CAJA;;AAAA;AAAA;AAIA,gBAAA,IAJA,uBAIA,IAJA;;AAKA;AACA,oBAAA,IAAA,KAAA,CAAA,EAAA;AACA;AACA,kBAAA,MAAA,CAAA,eAAA;AACA;;AATA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAUA;AA9GA,GAjBA;AAiIA,EAAA,OAjIA,qBAiIA;AACA,SAAA,eAAA,GADA,CACA;AACA;AAnIA,CAAA","sourcesContent":["<template>\r\n  <div class=\"account-list\">\r\n    <el-card class=\"box-card\">\r\n      <div slot=\"header\" class=\"clearfix\">\r\n        <span>员工列表</span>\r\n      </div>\r\n      <div>\r\n        <!-- 表格 -->\r\n        <el-table\r\n          @selection-change=\"handleSelectionChange\"\r\n          :data=\"tableData\"\r\n          ref=\"personalTable\"\r\n          tooltip-effect=\"dark\"\r\n          style=\"width: 100%\"\r\n        >\r\n          <!-- 选择框 -->\r\n          <el-table-column type=\"selection\" width=\"55\"></el-table-column>\r\n\r\n          <!-- ID -->\r\n          <el-table-column\r\n            prop=\"id\"\r\n            label=\"员工ID\"\r\n            width=\"160\"\r\n          ></el-table-column>\r\n          <!-- 姓名 -->\r\n          <el-table-column\r\n            prop=\"personalname\"\r\n            label=\"员工姓名\"\r\n            width=\"160\"\r\n          ></el-table-column>\r\n\r\n          <!-- 等级 -->\r\n          <el-table-column\r\n            prop=\"personalclass\"\r\n            label=\"员工等级\"\r\n            width=\"160\"\r\n          ></el-table-column>\r\n\r\n          <!-- 入职时间 -->\r\n          <el-table-column label=\"入职时间\" width=\"200\">\r\n            <template slot-scope=\"scope\">{{ scope.row.ctime }}</template>\r\n          </el-table-column>\r\n\r\n          <!-- 操作 -->\r\n          <el-table-column label=\"操作\">\r\n            <template slot-scope=\"scope\">\r\n              <el-button\r\n                size=\"mini\"\r\n                @click=\"handleEdit(scope.row)\"\r\n                type=\"primary\"\r\n                >编辑</el-button\r\n              >\r\n              <el-button size=\"mini\" @click=\"handleDel(scope.row)\" type=\"danger\"\r\n                >删除</el-button\r\n              >\r\n            </template>\r\n          </el-table-column>\r\n        </el-table>\r\n\r\n        <!-- 编辑的模态框 -->\r\n        <el-dialog\r\n          width=\"460px\"\r\n          title=\"员工编辑\"\r\n          :visible.sync=\"dialogFormVisible\"\r\n        >\r\n          <!-- 表单 -->\r\n          <el-form style=\"width: 337px;\" :model=\"editForm\">\r\n            <!-- 名字 -->\r\n            <el-form-item label=\"员工姓名\" label-width=\"120px\">\r\n              <el-input\r\n                v-model=\"editForm.personalname\"\r\n                autocomplete=\"off\"\r\n              ></el-input>\r\n            </el-form-item>\r\n\r\n            <!-- 员工等级 -->\r\n            <el-form-item item label=\"员工等级\" label-width=\"120px\">\r\n              <el-select\r\n                v-model=\"editForm.personalclass\"\r\n                placeholder=\"请选择员工等级\"\r\n              >\r\n                <el-option label=\"p1\" value=\"p1\"></el-option>\r\n                <el-option label=\"p2\" value=\"p2\"></el-option>\r\n                <el-option label=\"p3\" value=\"p3\"></el-option>\r\n                <el-option label=\"p4\" value=\"p4\"></el-option>\r\n                <el-option label=\"p5\" value=\"p5\"></el-option>\r\n                <el-option label=\"p6\" value=\"p6\"></el-option>\r\n                <el-option label=\"p7\" value=\"p7\"></el-option>\r\n              </el-select>\r\n            </el-form-item>\r\n          </el-form>\r\n\r\n          <div slot=\"footer\" class=\"dialog-footer\">\r\n            <el-button @click=\"dialogFormVisible = false\">取 消</el-button>\r\n            <el-button type=\"primary\" @click=\"saveEdit\">确 定</el-button>\r\n          </div>\r\n        </el-dialog>\r\n\r\n        <!-- 分页 -->\r\n        <div style=\"margin-top: 20px;\">\r\n          <el-pagination\r\n            @size-change=\"handleSizeChange\"\r\n            @current-change=\"handleCurrentChange\"\r\n            :current-page=\"currentPage\"\r\n            :page-sizes=\"[1, 3, 5, 10, 20]\"\r\n            :page-size=\"pageSize\"\r\n            layout=\"total, sizes, prev, pager, next, jumper\"\r\n            :total=\"total\"\r\n          ></el-pagination>\r\n        </div>\r\n\r\n        <!-- 批量删除 & 取消选择 -->\r\n        <div style=\"margin-top: 20px;\">\r\n          <el-button @click=\"batchDel\" size=\"small\" type=\"danger\"\r\n            >批量删除</el-button\r\n          >\r\n          <el-button @click=\"cancelSelect\" size=\"small\" type=\"primary\"\r\n            >取消选择</el-button\r\n          >\r\n        </div>\r\n      </div>\r\n    </el-card>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n// 引入时间处理模块\r\nimport moment from \"moment\";\r\n\r\n// 引入请求函数\r\nimport {\r\n  getPersonalList,\r\n  delPersonal,\r\n  batchdel,\r\n  editPersonal,\r\n} from \"@/api/personal\";\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      // 表格数据\r\n      tableData: [],\r\n      total: 0, // 数据总条数\r\n      currentPage: 1, // 当前页\r\n      pageSize: 3, // 每页条数\r\n      ids: [], // 被选中的数据的id们\r\n      // 编辑表单数据\r\n      editForm: {\r\n        personalname: \"\",\r\n        personalclass: \"\",\r\n      },\r\n      dialogFormVisible: false, // 控制编辑模态框的显示和隐藏\r\n    };\r\n  },\r\n  methods: {\r\n    // 取消选择\r\n    cancelSelect() {\r\n      this.$refs.personalTable.clearSelection();\r\n    },\r\n    // 请求员工列表\r\n    async getPersonalList() {\r\n      // 请求账号列表\r\n      let { total, data } = await getPersonalList({\r\n        currentPage: this.currentPage,\r\n        pageSize: this.pageSize,\r\n      });\r\n      // 如果当前页没有数据 且 当前页码不是第一页\r\n      if (!data.length && this.currentPage !== 1) {\r\n        this.currentPage -= 1; // 当前页码减去1\r\n        this.getPersonalList(); // 再次请求数据\r\n      }\r\n      // 对时间形成格式化处理\r\n      data.forEach(\r\n        (v) => (v.ctime = moment(v.ctime).format(\"YYYY-MM-DD hh:mm:ss\"))\r\n      );\r\n\r\n      // 赋值\r\n      this.total = total;\r\n      this.tableData = data;\r\n    },\r\n    // 每页条数改变\r\n    handleSizeChange(val) {\r\n      this.pageSize = val;\r\n      this.getPersonalList();\r\n    },\r\n    // 当前页码改变\r\n    handleCurrentChange(val) {\r\n      this.currentPage = val;\r\n      this.getPersonalList();\r\n    },\r\n    // 删除\r\n    handleDel(row) {\r\n      this.$confirm(\"你确定要删除吗?删除后无法恢复\", \"提示\", {\r\n        confirmButtonText: \"确定\",\r\n        cancelButtonText: \"取消\",\r\n        type: \"warning\",\r\n      })\r\n        // 确定\r\n        .then(async () => {\r\n          let { code } = await delPersonal({ id: row.id }); // 发送删除请求\r\n          // 成功\r\n          if (code === 0) {\r\n            // 刷新列表（重新请求列表数据）\r\n            this.getPersonalList();\r\n          }\r\n        })\r\n        // 取消\r\n        .catch(() => {\r\n          this.$message({\r\n            type: \"info\",\r\n            message: \"已取消删除\",\r\n          });\r\n        });\r\n    },\r\n    // 当选中状态改变\r\n    handleSelectionChange(rows) {\r\n      this.ids = rows.map((row) => row.id);\r\n    },\r\n    // 批量删除\r\n    batchDel() {\r\n      // 如果用户没有选中任何数据 不需要发送请求\r\n\r\n      if (!this.ids.length) {\r\n        this.$message.error(\"请选择以后再操作\");\r\n        return;\r\n      }\r\n\r\n      this.$confirm(\"你确定要删除吗？删除后无法恢复！\", \"提示\", {\r\n        confirmButtonText: \"确定\",\r\n        cancelButtonText: \"取消\",\r\n        type: \"warning\",\r\n      })\r\n        .then(async () => {\r\n          // 发送请求 把选中的id数组发送给后端\r\n          let { code } = await batchdel({ ids: JSON.stringify(this.ids) });\r\n          if (code === 0) {\r\n            this.getPersonalList(); // 刷新列表\r\n          }\r\n        })\r\n        .catch(() => {\r\n          this.$message({\r\n            type: \"info\",\r\n            message: \"已取消删除\",\r\n          });\r\n        });\r\n    },\r\n    // 编辑\r\n    handleEdit(row) {\r\n      // this.editForm = Object.assign({}, row);\r\n      this.editForm = { ...row }; // 赋值给编辑表单 回填数据\r\n      // 显示模态框\r\n      this.dialogFormVisible = true;\r\n    },\r\n    // 确定修改\r\n    async saveEdit() {\r\n      this.dialogFormVisible = false; // 隐藏模态框\r\n      // 结构出参数\r\n      let { id, personalname, personalclass } = this.editForm;\r\n      let { code } = await editPersonal({ id, personalname, personalclass });\r\n      // 成功\r\n      if (code === 0) {\r\n        // 刷新列表\r\n        this.getPersonalList();\r\n      }\r\n    },\r\n  },\r\n  created() {\r\n    this.getPersonalList(); // 进入页面 自动调用请求账号列表接口\r\n  },\r\n};\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n.el-button {\r\n  margin-left: 0;\r\n  margin-right: 10px;\r\n}\r\n</style>\r\n"],"sourceRoot":"src/views/PersonalManage"}]}