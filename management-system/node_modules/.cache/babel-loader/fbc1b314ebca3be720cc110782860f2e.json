,EAKA,aALA,yBAKA,aALA,EAMA,MANA,yBAMA,MANA,EAOA,SAPA,yBAOA,SAPA,EAQA,aARA,yBAQA,aARA,EAWA;;AACA,gBAAA,MAZA,GAYA;AACA,kBAAA,WAAA,EAAA,WADA;AAEA,kBAAA,SAAA,EAAA,SAFA;AAGA,kBAAA,aAAA,EAAA,aAHA;AAIA,kBAAA,MAAA,EAAA,MAJA;AAKA,kBAAA,SAAA,EAAA,SALA;AAMA,kBAAA,aAAA,EAAA;AANA,iBAZA;AAAA;AAAA,uBAoBA,SAAA,CAAA,MAAA,CApBA;;AAAA;AAAA;AAoBA,gBAAA,IApBA,oBAoBA,IApBA;AAqBA,gBAAA,OAAA,CAAA,GAAA,CAAA,IAAA,EArBA,CAsBA;;AACA,oBAAA,IAAA,KAAA,CAAA,EAAA;AACA,kBAAA,KAAA,CAAA,OAAA,CAAA,IAAA,CAAA,iBAAA;AACA;;AAzBA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AA0BA,KA5BA;AA8BA;AACA,IAAA,mBA/BA,+BA+BA,GA/BA,EA+BA,IA/BA,EA+BA;AAAA,UACA,IADA,GACA,GADA,CACA,IADA;AAAA,UACA,MADA,GACA,GADA,CACA,MADA;;AAEA,UAAA,IAAA,KAAA,CAAA,EAAA;AACA,aAAA,eAAA,CAAA,MAAA,GAAA,MAAA;AACA;AACA,KApCA;AAqCA;AACA,IAAA,kBAtCA,8BAsCA,IAtCA,EAsCA;AACA,UAAA,KAAA,GAAA,IAAA,CAAA,IAAA,KAAA,WAAA;AACA,UAAA,KAAA,GAAA,IAAA,CAAA,IAAA,KAAA,YAAA;AACA,UAAA,MAAA,GAAA,IAAA,CAAA,IAAA,GAAA,IAAA,GAAA,IAAA,GAAA,CAAA;;AAEA,UAAA,EAAA,KAAA,IAAA,KAAA,CAAA,EAAA;AACA,aAAA,QAAA,CAAA,KAAA,CAAA,uBAAA;AACA;;AACA,UAAA,CAAA,MAAA,EAAA;AACA,aAAA,QAAA,CAAA,KAAA,CAAA,mBAAA;AACA;;AACA,aAAA,CAAA,KAAA,IAAA,KAAA,KAAA,MAAA;AACA;AAlDA,GAlBA;AAuEA,EAAA,OAvEA,qBAuEA;AACA;AACA,QAAA,KAAA,GAAA,IAAA,CAAA,KAAA,CAAA,MAAA,CAAA,cAAA,CAAA,OAAA,CAAA,OAAA,CAAA,CAAA;AACA,SAAA,eAAA,CAAA,WAAA,GAAA,KAAA,CAAA,WAAA;AACA,SAAA,eAAA,CAAA,SAAA,GAAA,KAAA,CAAA,SAAA;AACA,SAAA,eAAA,CAAA,aAAA,GAAA,KAAA,CAAA,aAAA;AACA,SAAA,eAAA,CAAA,MAAA,GAAA,KAAA,CAAA,MAAA;AACA,SAAA,eAAA,CAAA,SAAA,GAAA,KAAA,CAAA,SAAA;AACA,SAAA,eAAA,CAAA,aAAA,GAAA,KAAA,CAAA,aAAA;AACA;AAhFA,CAAA","sourcesContent":["<template>\r\n  <div class=\"goods-edit\">\r\n    <el-card class=\"box-card\">\r\n      <div slot=\"header\">\r\n        <h1>货物信息修改</h1>\r\n      </div>\r\n      <div>\r\n        <!-- 货物表单 -->\r\n        <el-form\r\n          size=\"small\"\r\n          :model=\"arriveStoreForm\"\r\n          style=\"width: 560px;\"\r\n          ref=\"arriveStoreForm\"\r\n          label-width=\"80px\"\r\n        >\r\n          <!-- 名称 -->\r\n          <el-form-item label=\"货物码\">\r\n            <el-input v-model=\"arriveStoreForm.goodscoding\"></el-input>\r\n          </el-form-item>\r\n          <!-- 名称 -->\r\n          <el-form-item label=\"货物名称\">\r\n            <el-input v-model=\"arriveStoreForm.goodsname\"></el-input>\r\n          </el-form-item>\r\n          <!-- 分类 -->\r\n          <el-form-item label=\"货物分类\">\r\n            <el-select\r\n              v-model=\"arriveStoreForm.goodscategory\"\r\n              placeholder=\"请选择货物分类\"\r\n            >\r\n              <el-option label=\"酒水\" value=\"酒水\"></el-option>\r\n              <el-option label=\"食品\" value=\"食品\"></el-option>\r\n              <el-option label=\"日用品\" value=\"日用品\"></el-option>\r\n              <el-option label=\"衣服\" value=\"衣服\"></el-option>\r\n              <el-option label=\"器材\" value=\"器材\"></el-option>\r\n              <el-option label=\"图书\" value=\"图书\"></el-option>\r\n            </el-select>\r\n          </el-form-item>\r\n\r\n          <!-- 货物图片 -->\r\n          <el-form-item label=\"货物图片\">\r\n            <el-upload\r\n              class=\"avatar-uploader\"\r\n              action=\"http://127.0.0.1:5000/store/upload\"\r\n              :show-file-list=\"false\"\r\n              :on-success=\"handleAvatarSuccess\"\r\n              :before-upload=\"beforeAvatarUpload\"\r\n            >\r\n              <img\r\n                v-if=\"arriveStoreForm.imgUrl\"\r\n                :src=\"serverApi + arriveStoreForm.imgUrl\"\r\n                class=\"avatar\"\r\n              />\r\n              <i v-else class=\"el-icon-plus avatar-uploader-icon\"></i>\r\n            </el-upload>\r\n          </el-form-item>\r\n\r\n          <!-- 货物描述 -->\r\n          <el-form-item label=\"货物描述\">\r\n            <el-input\r\n              type=\"textarea\"\r\n              v-model=\"arriveStoreForm.goodsdesc\"\r\n            ></el-input>\r\n          </el-form-item>\r\n          <!-- 货物数量 -->\r\n          <el-form-item label=\"货物数量\">\r\n            <el-input v-model=\"arriveStoreForm.goodsquantity\"></el-input>\r\n          </el-form-item>\r\n          <!-- 确认按钮 -->\r\n          <el-form-item>\r\n            <el-button type=\"primary\" @click=\"submitForm\">确认修改</el-button>\r\n          </el-form-item>\r\n        </el-form>\r\n      </div>\r\n    </el-card>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { goodsEdit } from '@/api/store'\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      serverApi: 'http://127.0.0.1:5000/upload/goods/', // 商品图片服务器地址\r\n      // 商品表格\r\n      arriveStoreForm: {\r\n        // id: '', //货物ID\r\n        goodscoding: '', //货物码\r\n        goodsname: '', // 商品名称\r\n        goodscategory: '', // 商品分类\r\n        imgUrl: '', // 商品图片地址\r\n        goodsdesc: '', // 商品描述\r\n        goodsquantity: '', //货物数量\r\n      },\r\n\r\n      formLabelWidth: '120px', // 标签宽度\r\n    }\r\n  },\r\n  methods: {\r\n    // 提交添加表单\r\n    async submitForm() {\r\n      // 获取值\r\n      let {\r\n        goodscoding,\r\n        goodsname,\r\n        goodscategory,\r\n        imgUrl,\r\n        goodsdesc,\r\n        goodsquantity,\r\n      } = this.arriveStoreForm\r\n\r\n      // 准备参数对象\r\n      let params = {\r\n        goodscoding,\r\n        goodsname,\r\n        goodscategory,\r\n        imgUrl,\r\n        goodsdesc,\r\n        goodsquantity,\r\n      }\r\n      let { code } = await goodsEdit(params)\r\n      console.log(code)\r\n      // 发送添加商品的请求\r\n      if (code === 0) {\r\n        this.$router.push('/home/goodslist')\r\n      }\r\n    },\r\n\r\n    // 上传图片成功的回调\r\n    handleAvatarSuccess(res, file) {\r\n      let { code, imgUrl } = res\r\n      if (code === 0) {\r\n        this.arriveStoreForm.imgUrl = imgUrl\r\n      }\r\n    },\r\n    // 图片上传之前的处理函数\r\n    beforeAvatarUpload(file) {\r\n      const isPNG = file.type === 'image/png'\r\n      const isJPG = file.type === 'image/jpeg'\r\n      const isLt4M = file.size / 1024 / 1024 < 4\r\n\r\n      if (!(isPNG || isJPG)) {\r\n        this.$message.error('上传头像图片只能是 JPG/PNG 格式!')\r\n      }\r\n      if (!isLt4M) {\r\n        this.$message.error('上传头像图片大小不能超过 4MB!')\r\n      }\r\n      return (isPNG || isJPG) && isLt4M\r\n    },\r\n  },\r\n\r\n  created() {\r\n    // 获取本地存储的数据\r\n    let goods = JSON.parse(window.sessionStorage.getItem('goods'))\r\n    this.arriveStoreForm.goodscoding = goods.goodscoding\r\n    this.arriveStoreForm.goodsname = goods.goodsname\r\n    this.arriveStoreForm.goodscategory = goods.goodscategory\r\n    this.arriveStoreForm.imgUrl = goods.imgUrl\r\n    this.arriveStoreForm.goodsdesc = goods.goodsdesc\r\n    this.arriveStoreForm.goodsquantity = goods.goodsquantity\r\n  },\r\n}\r\n</script>\r\n\r\n<style lang=\"less\">\r\n.avatar-uploader .el-upload {\r\n  border: 1px dashed #d9d9d9;\r\n  border-radius: 6px;\r\n  cursor: pointer;\r\n  position: relative;\r\n  overflow: hidden;\r\n}\r\n.avatar-uploader .el-upload:hover {\r\n  border-color: #409eff;\r\n}\r\n.avatar-uploader-icon {\r\n  font-size: 28px;\r\n  color: #8c939d;\r\n  width: 178px;\r\n  height: 178px;\r\n  line-height: 178px;\r\n  text-align: center;\r\n}\r\n.avatar {\r\n  width: 178px;\r\n  height: 178px;\r\n  display: block;\r\n}\r\n</style>\r\n"],"sourceRoot":"src/views/StoreManage"}]}